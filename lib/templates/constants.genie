constant_Date : Ast.Value = {
    !inference {
        constant_date_point;
        constant_day_of_week;

        !turking {
            ["${duration} ago", weight=0.1]: (duration:constant_Measure_ms) => C.makeDate(null, '-', duration);
            ["${duration} before ${pt}", weight=0.01]: (duration:constant_Measure_ms, pt:constant_date_point) => C.makeDate(pt, '-', duration);

            ["{${duration} from now|in ${duration}}", weight=0.1]: (duration:constant_Measure_ms) => C.makeDate(null, '+', duration);
            ["${duration} after ${pt}", weight=0.01]: (duration:constant_Measure_ms, pt:constant_date_point) => C.makeDate(pt, '+', duration);

            ["${time} on ${date}"]: (time:constant_Time, date:constant_date_point) => C.makeDateWithDateTime(date, time);
            ["${date} ${time}"]: (time:at_time, date:constant_date_point) => C.makeDateWithDateTime(date, time);

            time:constant_Time => C.makeDateWithDateTime(null, time);
        }
    }
}

