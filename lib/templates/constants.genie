    ('jan' | 'january') [weight=0.1] => C.makeDate(C.dateOrDatePiece(null, 1, $loader), '+', null);
    ('jan' | 'january') ('of'|'') year:constant_Number => C.makeDate(C.dateOrDatePiece(year.value, 1, $loader), '+', null);
    ('feb' | 'february') [weight=0.1] => C.makeDate(C.dateOrDatePiece(null, 2, $loader), '+', null);
    ('feb' | 'february') ('of'|'') year:constant_Number => C.makeDate(C.dateOrDatePiece(year.value, 2, $loader), '+', null);
    ('mar' | 'march') [weight=0.1] => C.makeDate(C.dateOrDatePiece(null, 3, $loader), '+', null);
    ('mar' | 'march') ('of'|'') year:constant_Number => C.makeDate(C.dateOrDatePiece(year.value, 3, $loader), '+', null);
    ('apr' | 'april') [weight=0.1] => C.makeDate(C.dateOrDatePiece(null, 4, $loader), '+', null);
    ('apr' | 'april') ('of'|'') year:constant_Number => C.makeDate(C.dateOrDatePiece(year.value, 4, $loader), '+', null);
    'may' [weight=0.1] => C.makeDate(C.dateOrDatePiece(null, 5, $loader), '+', null);
    'may' ('of'|'') year:constant_Number => C.makeDate(C.dateOrDatePiece(year.value, 5, $loader), '+', null);
