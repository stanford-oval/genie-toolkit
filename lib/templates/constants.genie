    ('jun' | 'june') ('of'|'') year:constant_Number => C.makeDate(C.dateOrDatePiece(year.value, 6, $loader), '+', null);
    ('jul' | 'july') [weight=0.1] => C.makeDate(C.dateOrDatePiece(null, 7, $loader), '+', null);
    ('jul' | 'july') ('of'|'') year:constant_Number => C.makeDate(C.dateOrDatePiece(year.value, 7, $loader), '+', null);
    ('aug' | 'august') [weight=0.1] => C.makeDate(C.dateOrDatePiece(null, 8, $loader), '+', null);
