constant_date_point : Ast.Value = {
    'now'                 => C.makeDate(null, '+', null);
    'today'               => C.makeDate(new Ast.DateEdge('start_of', 'day'), '+', null);
    'yesterday'           => C.makeDate(new Ast.DateEdge('start_of', 'day'), '-', new Ast.Value.Measure(1, 'day'));

    !turking {
        'tomorrow'            => C.makeDate(new Ast.DateEdge('end_of', 'day'), '+', null);
        'the end of the day'  => C.makeDate(new Ast.DateEdge('end_of', 'day'), '+', null);
        'the end of the week' => C.makeDate(new Ast.DateEdge('end_of', 'week'), '+', null);
        'the end of the month'=> C.makeDate(new Ast.DateEdge('end_of', 'mon'), '+', null);
        'the end of the year' => C.makeDate(new Ast.DateEdge('end_of', 'year'), '+', null);
    }
}

constant_date_range : [Ast.Value, Ast.Value] = {
    'this week'   => [C.makeDate(new Ast.DateEdge('start_of', 'week'), '+', null), C.makeDate(new Ast.DateEdge('end_of', 'week'), '+', null)];
    'this month'  => [C.makeDate(new Ast.DateEdge('start_of', 'mon'), '+', null), C.makeDate(new Ast.DateEdge('end_of', 'mon'), '+', null)];
    'this year'   => [C.makeDate(new Ast.DateEdge('start_of', 'year'), '+', null), C.makeDate(new Ast.DateEdge('end_of', 'year'), '+', null)];

    !turking {
        'last week'  => [C.makeDate(new Ast.DateEdge('start_of', 'week'), '-', new Ast.Value.Measure(1, 'week'))!, C.makeDate(new Ast.DateEdge('start_of', 'week'), '+', null)];
        'last month' => [C.makeDate(new Ast.DateEdge('start_of', 'mon'), '-', new Ast.Value.Measure(1, 'mon'))!, C.makeDate(new Ast.DateEdge('start_of', 'mon'), '+', null)];
        'last year' => [C.makeDate(new Ast.DateEdge('start_of', 'year'), '-', new Ast.Value.Measure(1, 'year'))!, C.makeDate(new Ast.DateEdge('start_of', 'year'), '+', null)];

        'next week'  => [C.makeDate(new Ast.DateEdge('end_of', 'week'), '+', null), C.makeDate(new Ast.DateEdge('end_of', 'week'), '+', new Ast.Value.Measure(1, 'week'))!];
        'next month' => [C.makeDate(new Ast.DateEdge('end_of', 'mon'), '+', null), C.makeDate(new Ast.DateEdge('end_of', 'mon'), '+', new Ast.Value.Measure(1, 'mon'))!];
        'next year'  => [C.makeDate(new Ast.DateEdge('end_of', 'year'), '+', null), C.makeDate(new Ast.DateEdge('end_of', 'year'), '+', new Ast.Value.Measure(1, 'year'))!];

        // year
        year:constant_Number => {
            if (year.value <= 0)
                return null;
            return [
                C.makeDate(C.makeJSDate(C.fixTwoYearNumber(year.value), 1, $loader), '+', null),
                C.makeDate(C.makeJSDate(C.fixTwoYearNumber(year.value), 1, $loader), '+', new Ast.Value.Measure(1, 'year'))!
            ];
        };
        year:constant_Number 's' => {
            if (year.value <= 0)
                return null;
            return [
                C.makeDate(C.makeJSDate(C.fixTwoYearNumber(year.value), 1, $loader), '+', null),
                C.makeDate(C.makeJSDate(C.fixTwoYearNumber(year.value), 1, $loader), '+', new Ast.Value.Measure(10, 'year'))!
            ];
        };
    }

    // months
    ('jan' | 'january') => C.makeMonthDateRange(null, 1, $loader);
    ('jan' | 'january') ('of'|'') year:constant_Number => C.makeMonthDateRange(year.value, 1, $loader);
    ('feb' | 'february') => C.makeMonthDateRange(null, 2, $loader);
    ('feb' | 'february') ('of'|'') year:constant_Number => C.makeMonthDateRange(year.value, 2, $loader);
    ('mar' | 'march') => C.makeMonthDateRange(null, 3, $loader);
    ('mar' | 'march') ('of'|'') year:constant_Number => C.makeMonthDateRange(year.value, 3, $loader);
    ('apr' | 'april') => C.makeMonthDateRange(null, 4, $loader);
    ('apr' | 'april') ('of'|'') year:constant_Number => C.makeMonthDateRange(year.value, 4, $loader);
    'may' => C.makeMonthDateRange(null, 5, $loader);
    'may of' year:constant_Number => C.makeMonthDateRange(year.value, 5, $loader);
    ('jun' | 'june') => C.makeMonthDateRange(null, 6, $loader);
    ('jun' | 'june') ('of'|'') year:constant_Number => C.makeMonthDateRange(year.value, 6, $loader);
    ('jul' | 'july') => C.makeMonthDateRange(null, 7, $loader);
    ('jul' | 'july') ('of'|'') year:constant_Number => C.makeMonthDateRange(year.value, 7, $loader);
    ('aug' | 'august') => C.makeMonthDateRange(null, 8, $loader);
    ('aug' | 'august') ('of'|'') year:constant_Number => C.makeMonthDateRange(year.value, 8, $loader);
    ('sep' | 'sept' | 'september') => C.makeMonthDateRange(null, 9, $loader);
    ('sep' | 'sept' | 'september') ('of'|'') year:constant_Number => C.makeMonthDateRange(year.value, 9, $loader);
    ('oct' | 'october') => C.makeMonthDateRange(null, 10, $loader);
    ('oct' | 'october') ('of'|'') year:constant_Number => C.makeMonthDateRange(year.value, 10, $loader);
    ('nov' | 'november') => C.makeMonthDateRange(null, 11, $loader);
    ('nov' | 'november') ('of'|'') year:constant_Number => C.makeMonthDateRange(year.value, 11, $loader);
    ('dec' | 'december') => C.makeMonthDateRange(null, 12, $loader);
    ('dec' | 'december') ('of'|'') year:constant_Number => C.makeMonthDateRange(year.value, 12, $loader);
}

