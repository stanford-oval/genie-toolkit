{
  "name": "genie-toolkit",
  "version": "0.5.3",
  "description": "A Generator of Natural Language Parsers for Compositional Virtual Assistants",
  "main": "lib/index.js",
  "repository": "https://github.com/stanford-oval/genie-toolkit.git",
  "author": "Giovanni Campagna <gcampagn@cs.stanford.edu>",
  "license": "GPL-3.0+",
  "bin": {
    "genie": "./tool/genie.js"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "argparse": "^1.0.10",
    "body-parser": "^1.19.0",
    "byline": "^5.0.0",
    "csv-parse": "^4.4.5",
    "csv-stringify": "^5.3.0",
    "deep-equal": "^1.1.0",
    "en-inflectors": "^1.0.12",
    "en-pos": "^1.0.16",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "mmap-io": "^1.1.7",
    "morgan": "^1.9.1",
    "qs": "^6.7.0",
    "query-validation": "^0.1.0",
    "seedrandom": "^3.0.0",
    "sockaddr": "^1.0.1",
    "thingpedia": "~2.7.0-alpha.1",
    "thingtalk": "~1.10.0-alpha.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^6.0.0",
    "nyc": "^15.0.0",
    "pegjs": "^0.10.0"
  },
  "scripts": {
    "regen-meta-grammar": "pegjs ./lib/genie-compiler/grammar.pegjs",
    "lint": "eslint ./lib ./tool ./languages",
    "test": "nyc ./test/everything.sh",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepare": "yarn regen-meta-grammar"
  },
  "nyc": {
    "exclude": [
      "lib/genie-compiler/grammar.js",
      "test/**",
      "coverage/**"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "thingtalk",
      "thingpedia"
    ]
  }
}
